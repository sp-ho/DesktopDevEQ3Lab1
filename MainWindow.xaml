<Window x:Class="Lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="352" Width="448">
    <Grid>
        <Button x:Name="btnBank" Content="Bank Charges" HorizontalAlignment="Center" Margin="0,126,0,0" VerticalAlignment="Top" FontSize="20" Width="262" Click="btnBank_Click"/>
        <Button x:Name="btnShipping" Content="Fast Freight Shipping" HorizontalAlignment="Center" Margin="0,179,0,0" VerticalAlignment="Top" FontSize="20" Click="btnShipping_Click" Width="262"/>
        <Button x:Name="btnPopulation" Content="Population of Organisms" HorizontalAlignment="Center" Margin="0,233,0,0" VerticalAlignment="Top" FontSize="20" Click="btnPopulation_Click" Width="262"/>
        <Label x:Name="lblLab1" Content="Desktop Development Lab 1" HorizontalAlignment="Center" Margin="0,53,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
    </Grid>
</Window>
